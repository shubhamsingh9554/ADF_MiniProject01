{
	"name": "DL_MiniProject_Task1_Report",
	"properties": {
		"folder": {
			"name": "DL_MiniProject_Task1"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTablePrepUsers",
						"type": "DatasetReference"
					},
					"name": "users01"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTablePrepAdmins",
						"type": "DatasetReference"
					},
					"name": "admins01"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTableResult",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "union1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          email as string,",
				"          name as string,",
				"          password as string,",
				"          role as string,",
				"          department as string,",
				"          reportsTo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> users01",
				"source(output(",
				"          email as string,",
				"          name as string,",
				"          password as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> admins01",
				"users01, admins01 union(byName: true)~> union1",
				"union1 filter(!isNull(email) || !isNull(password)) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['email'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}